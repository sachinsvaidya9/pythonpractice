    " # Guess the number!\n",
    "    \n",
    "import random\n",
    "\n",
    "number = random.randint(1, 10)\n",
    "guess = int(input(\"Guess a number between 1 and 10: \"))\n",
    "\n",
    "if guess == number:\n",
    "    print(\"You guessed it! The number was\", number)\n",
    "else:\n",
    "    print(\"Sorry, the number was\", number, \". Try again!\")\n"
   ]
